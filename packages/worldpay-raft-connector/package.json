{
  "name": "@gradientedge/worldpay-raft-connector",
  "version": "1.0.0",
  "description": "Worldpay RAFT connector for commercetools",
  "author": "Gradient Edge team <info@gradientedge.com>",
  "homepage": "https://github.com/gradientedge/TODO#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "files": ["dist", "!dist/**/__tests__"],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/gradientedge/TODO.git"
  },
  "scripts": {
    "clean": "rimraf dist && rm -f tsconfig.tsbuildinfo",
    "build": "tsc --build",
    "build:watch": "tsc --build --incremental --watch",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --cache --check \"**/*.{ts,js,json}\"",
    "prettify": "prettier --cache --write \"**/*.{ts,js,json}\"",
    "test": "jest --config jest.config.cjs",
    "test:coverage": "jest --config jest.config.cjs --coverage --collectCoverageFrom='src/**/*.{ts,jxs}'"
  },
  "bugs": {
    "url": "https://github.com/gradientedge/commercetools-connect-poc/issues"
  },
  "dependencies": {
    "@commercetools/platform-sdk": "7.9.0",
    "@commercetools/sdk-client-v2": "2.5.0",
    "@gradientedge/logger": "4.0.0",
    "@gradientedge/worldpay-raft-messages": "file:../worldpay-raft-messages",
    "@reflet/http": "1.0.0",
    "axios": "1.6.5",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.29",
    "@tsconfig/recommended": "^1.0.7",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2"
  }
}
